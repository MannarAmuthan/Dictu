import BigInt;

{ // main
    const bi1 = BigInt.new("9").unwrap();
    const bi2 = BigInt.new(8987657899).unwrap();
    print(bi1.compare(bi2));

    print(bi1.negate().unwrap());
    print(bi1);
    print(bi1.abs().unwrap());

    print(bi1.bitwiseAnd(bi2).unwrap());
    print(bi1.bitwiseOr(bi2).unwrap());
}