/**
* __file__.du
*
* Testing __file__
*/
import Path;
import System;

assert(Path.basename(__file__) == "__file__.du");
if (System.platform == "windows") {
    assert(Path.dirname(__file__) == System.getCWD().unwrap() + r"\tests\builtins");
} else {
    assert(Path.dirname(__file__) == System.getCWD().unwrap() + "/tests/builtins");
}
